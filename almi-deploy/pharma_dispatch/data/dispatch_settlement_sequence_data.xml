<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Secuencia para Liquidaciones -->
        <record id="seq_dispatch_settlement" model="ir.sequence">
            <field name="name">Liquidaci√≥n de Planilla</field>
            <field name="code">dispatch.settlement</field>
            <field name="prefix">LIQ/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="use_date_range">True</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Secuencia para Hojas de Cobranzas -->
        <record id="seq_dispatch_collection_sheet" model="ir.sequence">
            <field name="name">Hoja de Cobranzas</field>
            <field name="code">dispatch.collection.sheet</field>
            <field name="prefix">HC/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="use_date_range">True</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>

